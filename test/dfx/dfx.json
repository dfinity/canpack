{
    "canisters": {
        "service": {
            "dependencies": ["mo_rust"],
            "type": "motoko",
            "main": "src/Main.mo"
        },
        "mo_rust": {
            "type": "rust",
            "package": "mo_rust",
            "candid": ".mo-rust/service.did",
            "gzip": true
        }
    },
    "defaults": {
        "build": {
            "packtool": "npx --no ic-mops sources"
        }
    }
}
